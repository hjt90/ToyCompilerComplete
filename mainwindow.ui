int b;

int program(int a, int b, int c)
{
	int i;
	int j;
	i = 0;
	if (a &gt; (b + c))
	{
		j = a + (b * c + 1);
	}
	else
	{
		j = a;
	}
	while (i &lt;= 100)
	{
		i = j * 2;
		j = j * 2;
	}
	return i;
}

int demo(int a)
{
	a = a + 2;
	return a * 2;
}

