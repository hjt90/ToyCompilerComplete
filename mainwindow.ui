{
	int a[2][2];
	a[0][0] = 3;
	a[0][1] = a[0][0] + 1;
	a[1][0] = a[0][0] + a[0][1];
	a[1][1] = program(a[0][0], a[0][1], demo(a[1][0]));
	return a[1][1];
}
